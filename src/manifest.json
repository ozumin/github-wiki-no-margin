{
  "name": "NoMargin",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Chrome Extension For Github",
  "browser_action": {
    "name": "Click to get URL"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["https://github.com/*/*/wiki/*", "https://github.com/*/*/wiki"]
}
